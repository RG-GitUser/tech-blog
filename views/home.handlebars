

<h1>Welcome to My Blog!</h1>



{{#if blogPosts.length}}
    <h2>Latest Blog Posts</h2>
    <ul>
        {{#each blogPosts}}
            <li>{{this.title}}</li>
        {{/each}}
    </ul>
{{else}}
    <p>No blog posts yet.</p>
{{/if}}



<script src="https://cdn.jsdelivr.net/npm/handlebars@4.7.7/dist/handlebars.min.js"></script>
<script src="./js/home.js"></script>